{
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "types": ["node", "vite/client"],
        "allowJs": false,
        "isolatedModules": true,
        "jsx": "react-jsx",
        //"downlevelIteration": true
        "module": "ESNext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    },
    "include": ["**/*.tsx"]
}
